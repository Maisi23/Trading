    "def max_drawdown_in_R(data:pd.DataFrame):\n",
    "    if \"R\" not in data.columns: \n",
    "        add_R(data)\n",
    "        print(\"Columna R añadida\")\n",
    "    \n",
    "    data[\"R\"]= data[\"R\"].fillna(0)\n",
    "\n",
    "    balance_n = data[\"R\"].cumsum() \n",
    "    peak = balance_n.cummax()\n",
    "    dd_in_R = balance_n - peak\n",
    "    return dd_in_R.min() \n",
